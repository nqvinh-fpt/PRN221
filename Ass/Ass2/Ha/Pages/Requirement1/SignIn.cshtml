@page
@model SignInModel

<!DOCTYPE html>
<html>
<head>
    <title>Sign In</title>
</head>
<body>
    <h1>Sign In</h1>
    <form method="post">
        <div>
            <label for="Username">Username:</label>
            <input type="text" id="Username" name="Username" />
        </div>
        <div>
            <label for="Password">Password:</label>
            <input type="password" id="Password" name="Password" />
        </div>
        <button type="submit">Sign In</button>
    </form>
</body>
</html>