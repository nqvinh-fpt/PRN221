@page
@model NQVinh_Assignment03.Pages.Events.ViewCountModel
<h1>Events</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Title
            </th>
            <th>
                Count
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Events)
        {
                <tr>
                    <td>
                    @item.Title
                    </td>
                    <td>
                    @item.Attendees.Count
                    </td>
                </tr>
        }
    </tbody>
</table>

